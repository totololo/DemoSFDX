    <validationRules>
        <fullName>CONT_VR01_Email_Obligatoire_contact_oper</fullName>
        <active>true</active>
        <description>Lorsqu&apos;un contact est ajouté en tant que contact opérationnel, son email est obligatoire.</description>
        <errorConditionFormula>AND(!$User.BypassValidationRule__c, Contact_operationnel__c  = true,ISBLANK(Email))</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Vous devez impérativement indiquer une adresse mail s&apos;il s&apos;agit d&apos;un contact opérationnel.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CONT_VR02_Prenom_obligatoire</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!$User.BypassValidationRule__c,
ISBLANK( FirstName ))</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>Vous devez obligatoirement renseigner le champ &quot;Prénom&quot;</errorMessage>
    </validationRules>
